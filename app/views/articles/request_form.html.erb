<%= form_with url: '/articles/request' do |form| %>
<%= form.label :startDate, 'Start Date' %>
<%= form.text_field :startDate %>
<%= form.label :endDate, 'End Date' %>
<%= form.text_field :endDate %>
<%= form.label :keywords %>
<%= form.text_field :keywords %>
<%= form.label :amount %>
<%= form.text_field :amount %>
<%= form.submit %>
<% end %>
