<%= form_with url: '/articles/request' do |form| %>
<%= form.label :startDate, 'Start Date' %>
<%= form.text_field :startDate %><br>
<%= form.label :endDate, 'End Date' %>
<%= form.text_field :endDate %><br>
<%= form.label :keywords %>
<%= form.text_field :keywords %><br>
<%= form.label :amount %>
<%= form.text_field :amount %><br>
<%= form.submit %>
<% end %>
